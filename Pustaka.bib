@inproceedings{Izziati2019,
	abstract = {The Indonesian mortality table is still based on the observed factors, e.g., age and sex. According to the data, mortality is also based on the unobserved factors, e.g., health index, behavior, or background of the population. In this paper, a health index called physiological ages was introduced to improve the Indonesian mortality table. This study uses a finite state with one absorbing state Markov aging process and time until death following a phase type distribution. The result is a renewal of the Indonesian mortality table based on age, sex, and a health index.},
	author = {I. A. Izziati and D. Lestari and S. Devila},
	doi = {10.1063/1.5132464},
	isbn = {9780735419155},
	issn = {15517616},
	journal = {AIP Conference Proceedings},
	keywords = {Markov chain,mortality table,phase type distribution,physiological age},
	month = {11},
	publisher = {American Institute of Physics Inc.},
	title = {Indonesian mortality table with Markov aging process},
	volume = {2168},
	year = {2019},
}
